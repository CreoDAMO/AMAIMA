# =============================================================================
# AMAIMA - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your values.
# Do NOT commit .env to version control.

# =============================================================================
# Database (Required)
# =============================================================================
DATABASE_URL=postgresql://user:password@host:5432/dbname

# =============================================================================
# NVIDIA NIM API (Required for AI inference)
# =============================================================================
NVIDIA_API_KEY=nvapi-xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# Backend Configuration
# =============================================================================
BACKEND_URL=http://localhost:8000
AMAIMA_EXECUTION_MODE=execution-enabled

# NIM API Endpoints (defaults shown, override if using custom endpoints)
COSMOS_NIM_URL=https://integrate.api.nvidia.com/v1
BIONEMO_NIM_URL=https://integrate.api.nvidia.com/v1

# =============================================================================
# Frontend Configuration
# =============================================================================
NEXT_PUBLIC_API_URL=http://localhost:8000

# =============================================================================
# Security
# =============================================================================
API_SECRET_KEY=your-api-secret-key-here
SESSION_SECRET=your-session-secret-here

# =============================================================================
# Stripe (Managed by Replit integration - no manual config needed)
# =============================================================================
# Stripe keys are automatically provided via Replit's Stripe integration.
# In production, you'll add live keys (pk_live_xxx, sk_live_xxx) when publishing.
# STRIPE_WEBHOOK_SECRET is set automatically by stripe-replit-sync.
